{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/admin(.*)",
            "destination": "/admin.html"
        },
        {
            "source": "/((?!api/|admin).*)",
            "destination": "/index.html"
        }
    ]
}